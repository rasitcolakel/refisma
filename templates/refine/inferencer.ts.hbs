import React from 'react'
import { {{inferencer.component}} } from '{{inferencer.package}}'
{{{fieldTransformer}}}

const fieldTransformer = (field:any) => {
    const f= fields.find((f) => f.key === field.key);
    return f || field;
}

export default function {{capitalize action}}() {
    return (
        <{{inferencer.component}}
            resource="{{lowercasePlural name}}"
            action="{{action}}"
            fieldTransformer={fieldTransformer}
        />
    )
}