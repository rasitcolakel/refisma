#my elementTypes are number, text, date and checkbox on mui 
{{#ifEquals this.elementType "number"}}
    <TextField
        {...register("{{this.name}}", {
            required: "This field is required",
        })}
        error={!!(errors as any)?.{{this.name}} }
        helperText={(errors as any)?.{{this.name}}?.message}
        margin="normal"
        fullWidth
        type="{{this.elementType}}"
        label={t('table.{{this.name}}')}
        name="{{this.name}}"
    />
{{else}}
 